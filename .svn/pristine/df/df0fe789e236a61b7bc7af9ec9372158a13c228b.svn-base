public class Board
{
    private final int width;
    private final int height;

    private static Piece[][] board = null;

    public Board(){
	this.width = 8;
	this.height = 8;
	board = new Piece[8][8];
    }

    public static Piece[][] getBoard() {
	return board;
    }

    public Piece getPieceAt(int x, int y){
	return board[x][y];

    }
}
